{
    "name": "fullstack",
    "version": "1.0.0",
    "description": "srm system",
    "main": "index.js",
    "engines": {
       "node": "12.12.0",
        "npm": "6.11.3"
    },
    "scripts": {
        "start": "node index",
        "server": "nodemon index",
        "client-install": "npm install --prefix client",
        "client": "npm run start --prefix client",
        "dev": "concurrently \"npm run server\" \"npm run client\"",
        "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build --prefix client"
    },
    "keywords": [
        "express",
        "angular"
    ],
    "author": "Dmitri Fedorets",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.1",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "mongoose": "^5.7.8",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0"
    },
    "devDependencies": {
        "concurrently": "^5.0.0",
        "nodemon": "^1.19.4"
    }
}
